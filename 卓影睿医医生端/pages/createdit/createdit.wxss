page {
  font-family: MicrosoftYahei;
  height: 100%;
}
.editbackground{
  width: 100%;
  position: absolute;
  top: 0;
  background-color:rgb(241,242,243);
}
.editcontent{
  background-color: rgb(241,242,243);
  width: 100%;
  /* margin-top: 71rpx; */
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.details{
  background-color: white;
  height: 336rpx;
  width: 690rpx;
  border-radius: 6rpx;
  margin-top: 30rpx;
}
.head {
  font-size: 28rpx;
  color: rgb(96,98,102);
  margin-right: 30rpx;
}

.frehead {
  font-size: 28rpx;
  color: rgb(96,98,102);
}

.unit {
  font-size: 28rpx;
  color: rgb(96,98,102);
  margin-left: 30rpx;
}

.cycle {
  width: 650rpx;
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.ui-radio radio {
  display: none;
}

.label {
  margin-left: 30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.ui-radio {
  width: 60rpx;
  height: 40rpx;
  border: 1rpx solid rgb(96,98,102);
  border-radius: 20rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  margin-right: 15rpx;
}

.ui-radio.active {
  background-color: rgb(60,76,193);
  color: #fff;
  border-radius: 20rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  margin-right: 15rpx;
}

.fre-radio radio {
  display: none;
}

.fre-radio {
  width: 80rpx;
  height: 40rpx;
  border: 1rpx solid rgb(96,98,102);
  border-radius: 20rpx;
