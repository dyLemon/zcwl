  margin-right: 15rpx;
}

.fre-radio.active {
  background-color: rgb(60,76,193);
  color: #fff;
  border-radius: 20rpx;
  text-align: center;
  line-height: 36rpx;
  font-size: 24rpx;
  margin-right: 15rpx;
}

