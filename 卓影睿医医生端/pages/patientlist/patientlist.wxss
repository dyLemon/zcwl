page {
    font-family: MicrosoftYaHeiLight;
    height: 100%;
}

.background {
    height: 100%;
}
.content{
    position: absolute;
    width: 100%;
    top: 274rpx;
    border-top-left-radius:10rpx ;
    border-top-right-radius:10rpx ;
    background-color: white;
}
.bottom{
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 6px;
    background: linear-gradient(rgb(255,255,255),rgb(233,233,233));
  }
  .bline{
    position: absolute;
    bottom: 0;
      width: 100%;
      border-bottom: 1px solid rgb(204,204,204);
  }
.empty {
    position: relative;
    top:-140rpx;
    height: 100%;
    align-items: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.epimg {
    position: relative;
    left: 35rpx;
    width: 209rpx;
    height: 137rpx;
}

.emptytext{
    margin-top: 20rpx;
    font-size: 24rpx;
    color: rgb(193,193,193);
}
.title {
    background-image: url(https://wxprogram.zc5l.com/img/list_user_bg.png);
    background-size: 100% 100%;
    height: 168px;
    display: flex;
}

.b {
    top: 234rpx;
    width: 500rpx;
    height: 80rpx;
    background-color: white;
    border: none;
    border-radius: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 1px solid rgba(181, 181, 181, 0.425);
    z-index: 1;
}
.b::after{
    display: none;
}

.hb {
    background-color: #7cb5fb;
}

.hb text {
    color: white;
}

.b image {
    width: 36rpx;
    height: 36rpx;
    margin: 8rpx;
}

.b text {
    color: rgb(181,181,181);
    font-size: 24rpx;
}

.tablehead {
    display: flex;
    width: 100%;
    flex-direction: row;
    justify-content:space-around;
    height: 140rpx;
    font-size: 30rpx;
    color: rgb(60,76,193);
    border-bottom: 1px solid rgba(60, 76, 193, 0.384);
}
.tabletitle{
    width: 120rpx;
    margin-top: 70rpx;
}
.table {
    display: flex;
    justify-content:space-around;
    border-bottom: 1px solid rgb(238,238,238);
    font-size: 26rpx;
    color: rgb(144,147,153);
}
.tableh{
    background-color: rgb(241,247,255);
    display: flex;
    border-bottom: 2px solid rgb(238,238,238);
    font-size: 26rpx;
    color: rgb(144,147,153);
}
.tbcontent{
    width: 620rpx;
    display: flex;
    justify-content:space-between;
}
.patientbox{
    position: relative;
    left: -20rpx;
    width: 200rpx;
    display: flex;
    align-items: center;
}
.avatar{
    width: 60rpx;
    height: 60rpx;
    background-size: 100% 100%;
}
.namebox{
    margin-left: 20rpx;
    display: flex;
    flex-direction: column;
}
.name {
    font-size: 30rpx;
    color: rgb(34,34,34);
}
.sexage{
    font-size: 24rpx;
    color: rgb(119,119,119);
}
.time {
    position: relative;
    left: -18rpx;
    width: 170rpx;
    font-size: 28rpx;
    color: rgb(34,34,34);
    line-height: 120rpx;
    padding-right: 10rpx;
}

.times {
    width: 120rpx;
    font-size: 28rpx;
    color: rgb(34,34,34);
    text-align: center;
    line-height: 120rpx;
}
