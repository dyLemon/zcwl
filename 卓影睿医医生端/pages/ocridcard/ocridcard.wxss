page {
  height: 100%;
  width: 100%;
  font-family: MicrosoftYahei;
}
.background{
  width: 100%;
  height: 100%;
  background-color: rgb(241, 242, 243);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.titlebg{
  width: 100%;
  height: 304rpx;
  background-image: url(https://wxprogram.zc5l.com/img/ocr_top.png);
  background-size: 100% 100%;
}
.contentbg{
  width:716rpx;
  background-color: white;
  border-bottom-left-radius:8rpx;
  border-bottom-right-radius:8rpx;
}
.rowboxt{
  height: 127rpx;
  border-bottom:1px solid rgb(204, 204, 204);
  display: flex;
  align-items: center;
}
.property{
  font-size: 28rpx;
  color: rgb(102, 102, 102);
  margin-left: 97rpx;
  width: 120rpx;
}
.content{
  font-size: 28rpx;
  color: rgb(34, 34, 34);
  margin-left: 50rpx;
}
.icon{
  position: absolute;
  width: 269rpx;
  height: 200rpx;
  bottom: 0;
  right: 0;
  background-image: url(https://wxprogram.zc5l.com/img/success_icon.png);
  background-size: 100% 100%;
}