page{
  font-family: MicrosoftYahei;
  height: 100%;
}
.background{
  height: 100%;
  width: 100%;
  background-color: rgb(248,248,248);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.titlebox{
  width: 100%;
  height: 109rpx;
  display: flex;
  align-items: center;
  border-radius: 6rpx;
}
.name{
  width: 100rpx;
  margin-left: 30rpx;
  font-size: 30rpx;
  color: rgb(34, 34, 34);
  text-overflow:ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.dt{
  margin-left: 40rpx;
  font-size: 24rpx;
  color: rgb(96,98,102);
}
.dname{
  margin-left: 28rpx;
  font-size: 30rpx;
  color: rgb(34, 34, 34);
}
.bdname{
  margin-left: 28rpx;
  font-size: 30rpx;
  color: rgb(34, 34, 34);
  width: 380rpx;
  text-overflow:ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.deploybox{
  width:720rpx;
  height: 134rpx;
  background-color: rgb(241,242,243);
  display: flex;
  align-items: center;
}
.talebox{
  width: 240rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 90rpx;
}
.property{
  font-size: 28rpx;
  color: rgb(34, 34, 34);
}
.value{
  margin-top: 12rpx;
  font-size: 24rpx;
  color: rgb(60,76,193);
}
.dtline{
  height: 90rpx;
  border-left: 1px solid rgb(238,238,238);
}
.lb{
  width: 100%;
  background-color: white;
}
.vshot{
  width: 140rpx;
  height: 140rpx;
  margin-left: 60rpx;
}
.doshadow{
  width: 750rpx;
  height: 6px;
  background: linear-gradient(rgba(241,242,243,1),rgba(124,157,243,0.2));
  position: absolute;
  top:231rpx;
}

.bshadow{
  width: 100%;
  height: 6px;
  background: linear-gradient(rgba(124,157,243,0.2),rgba(255,255,255,1));
  position: absolute;
  bottom: 140rpx;
}
.footmark {
  border-style: solid;
  box-shadow:0px 0px 4px 0px rgb(238,238,238);
  background-color: white;
  border-width: 0rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 140rpx;
  position: absolute;
  bottom: 0;
}
.fbox{
  position: fixed;
  left: 24rpx;
  width: 696rpx;
  display: flex;
  justify-content: space-between;
}
.ftt {
  width: 300rpx;
  height: 93rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.scicon{
  width: 80rpx;
  height: 80rpx;
  background-image: url(https://wxprogram.zc5l.com/img/shoppingcar_icon_n.png);
  background-size: 100% 100%;
}
.sciconh{
  width: 80rpx;
  height: 80rpx;
  background-image: url(https://wxprogram.zc5l.com/img/shoppingcar_icon_p.png);
  background-size: 100% 100%;
}
.tbox{
  margin-left: 20rpx;
  height: 80rpx;
  display: flex;
  flex-direction: column;
}
.pricebox{
  display: flex;
  font-size: 30rpx;
  color: rgb(209,72,73);
}
.p{
  margin-left: 12rpx;
}
.tcount{
  font-size: 24rpx;
  color: rgb(96,98,102);
}
.comfirm {
  margin: 0;
  padding: 0;
  width: 400rpx;
  height: 80rpx;
  border-radius: 80rpx;
  background-color: rgb(60,76,193);
  color: white;
  line-height: 80rpx;
}

.comfirmh {
  background-color: #7cb5fb;
}