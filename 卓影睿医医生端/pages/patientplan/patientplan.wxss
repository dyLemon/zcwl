page {
    font-family: MicrosoftYaHeiLight;
    height: 100%;
}

.background {
    background-color: white;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.empty {
    height: 980rpx;
    align-items: center;
    display: flex;
    justify-content: center;
}

.epimg {
    position: relative;
    left: 35rpx;
    width: 209rpx;
    height: 137rpx;
}

.title {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.name {
    color: rgb(34, 34, 34);
    font-size: 30rpx;
}

.property {
    margin-left: 38rpx;
    font-size: 24rpx;
    color: rgb(34, 34, 34);
    word-break: keep-all;
}

.left {
    margin-top: 30rpx;
    margin-bottom: 30rpx;
    width: 690rpx;
    display: flex;
    
    align-items: center;
}

.infobox {
    width: 680rpx;
    border-radius: 4rpx;
    background-color: rgb(246, 246, 246);
}

.ibox {
    margin-top: 30rpx;
    width: 630rpx;
    display: flex;
}

.info {
    margin-left: 30rpx;
    width: 497rpx;
    font-size: 24rpx;
    color: rgb(96, 98, 102);
}

.dproperty {
    margin-left: 38rpx;
    font-size: 24rpx;
    color: rgb(34, 34, 34);
    word-break: keep-all;
}

.dinfo {
    margin-left: 30rpx;
    width: 497rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    color: rgb(96, 98, 102);
    word-break: break-all;
}

.ibbox {
    margin-top: 30rpx;
    width: 630rpx;
    display: flex;
    margin-bottom: 30rpx;
}

.tline {
    margin-top: 30rpx;
    width: 100%;
    border-bottom: 4px solid rgb(238, 238, 238);
}

.talk {
    position: absolute;
    right: 40rpx;
    width: 100rpx;
    height: 40rpx;
    padding: 0;
    background: none;
    display: flex;
}

.talk image {
    width: 40rpx;
    height: 40rpx;
    background-size: 100%;
    background-image: url(https://wxprogram.zc5l.com/img/titile_next_icon_n.png);
}

.talk:after {
    border: none;
}

.talkh {
    background: none;
    padding: 0;
}

.talkh image {
    width: 40rpx;
    height: 40rpx;
    background-size: 100%;
    background-image: url(https://wxprogram.zc5l.com/img/titile_next_icon_p.png);
}

.create {
    width: 690rpx;
    height: 80rpx;
    border-radius: 10rpx;
    font-size: 30rpx;
    line-height: 80rpx;
    color: white;
    background-color: rgb(60, 76, 193);
}

.createh {
    background-color: #7cb5fb;
}

.table {
    position: relative;
    width: 690rpx;
    border-bottom: 1rpx solid rgb(243, 241, 241);
    margin-top: 30rpx;
}

.remove {
    width: 128rpx;
    height: 100%;
    background-color: rgb(241, 242, 243);
    color: rgb(96, 98, 102);
    position: absolute;
    top: 0;
    right: -128rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.ttitle {
    display: flex;
    padding-left: 30rpx;
    padding-right: 30rpx;
    align-items: center;
}

.tname {
    font-size: 28rpx;
    color: rgb(96, 98, 102);
    margin-right: 10rpx;
}

.ttime {
    font-size: 24rpx;
    color: rgb(144, 147, 153);
    line-height: 30rpx;
}

.ttimem {
    margin-left: 20rpx;
    font-size: 24rpx;
    color: rgb(144, 147, 153);
    line-height: 30rpx;
}

.codeb {
    background-image: url(https://wxprogram.zc5l.com/img/list_code_n.png);
    background-size: 100% 100%;
    position: fixed;
    right: 30rpx;
    width: 150rpx;
    height: 60rpx;
}

.codebh {
    background-image: url(https://wxprogram.zc5l.com/img/list_code_p.png);
    background-size: 100% 100%;
    position: fixed;
    right: 30rpx;
    width: 150rpx;
    height: 60rpx;
}

.delete {
    position: absolute;
    right: 30rpx;
    width: 120rpx;
    height: 40rpx;
    background-size: 100%;
    background-image: url(https://wxprogram.zc5l.com/img/delet_icon_n.png);
    z-index: 1;
}

.deleteh {
    background-size: 100%;
    background-image: url(https://wxprogram.zc5l.com/img/delet_icon_p.png);
}

.tshow {
    display: flex;
    margin: 20rpx;
    height: 200rpx;
}

.tshow image {
    width: 222rpx;
    height: 126rpx;
}

.tdetails {
    margin-left: 20rpx;
    width: 428rpx;
}

.action {
    font-size: 24rpx;
    color: rgb(96, 98, 102);
    margin-bottom: 13rpx;
}

.count text {
    padding-left: 10rpx;
    padding-right: 10rpx;
    padding-bottom: 5rpx;
    padding-top: 5rpx;
    background-color: rgb(246, 246, 246);
    color: rgb(144, 147, 153);
    border-radius: 10rpx;
    font-size: 20rpx;
}

.price {
    margin-top: 20rpx;
    font-size: 20rpx;
    color: rgb(209, 72, 73);
}

.statebox {
    width: 428rpx;
    position: absolute;
    justify-content: flex-end;
    display: flex;
    align-items: center;
}

.state {
    margin-left: 15rpx;
    width: 80rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 20rpx;
    color: rgb(144, 147, 153);
}

.pay {
    border-radius: 20rpx;
    padding: 0;
    width: 130rpx;
    height: 40rpx;
    line-height: 36rpx;
    font-size: 20rpx;
    color: rgb(209, 72, 73);
    border: 1rpx solid rgb(209, 72, 73);
}

.pay::after {
    display: none;
}

.payh {
    color: #ff8a8b;
    border: 1rpx solid #ff8a8b;
}

.qrback {
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 1000;
}

.qrbox {
    background-color: white;
    width: 550rpx;
    height: 700rpx;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.qrtitle {
    font-size: 34rpx;
    color: rgb(96, 98, 102);
    margin-top: 90rpx;
    margin-bottom: 60rpx;
}

.qrbg {
    background-color: rgb(248, 248, 248);
    width: 350rpx;
    height: 350rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.qrdes {
    font-size: 24rpx;
    color: rgb(193, 193, 193);
    margin-top: 50rpx;
    line-height: 40rpx;
}

.background .seeTable {
    position: absolute;
    left: 20rpx;
    bottom: 40rpx;
    font-size: 20rpx;
    color: rgb(60, 76, 193);

}

.footbox {
    position: fixed;
    display: flex;
    bottom: 0;
    height: 140rpx;
    width: 100%;
    border-top: 1px solid rgb(238, 238, 238);
    align-items: center;
    justify-content: center;
}

.talkbtn {
    width: 220rpx;
    height: 80rpx;
    background-image: url(https://wxprogram.zc5l.com/img/chant_btn_n.png);
    background-size: 100% 100%;
}

.talkbtnh {
    width: 220rpx;
    height: 80rpx;
    background-image: url(https://wxprogram.zc5l.com/img/chant_btn_p.png);
    background-size: 100% 100%;
}

.createbtn {
    margin-left: 20rpx;
    text-align: center;
    width: 450rpx;
    height: 80rpx;
    color: white;
    font-size: 30rpx;
    line-height: 80rpx;
    background-color: rgb(60, 76, 193);
    border-radius: 80rpx;
}

.createbtnh {
    width: 450rpx;
    height: 80rpx;
    width: 450rpx;
    height: 80rpx;
    color: white;
    font-size: 30rpx;
    line-height: 80rpx;
    background-color: #7cb5fb;
    border-radius: 80rpx;
}