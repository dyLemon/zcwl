.wrapper{
  position:relative;
  width:100%;
  background-color:rgb(0, 0, 0);
  overflow: hidden;
}
.wrapper image{
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  z-index:5;
  /* margin-top:40rpx; */
}
.canvas-one{
  position:absolute;
  top:50%;
  left:-50%;
  transform: translate(-50%,-50%);
  /* width:90%; */
  margin:0 auto;
  vertical-align: middle;
}
.canvas-wrapper{
  position:relative; 
  width:100%;
  /* margin-left:750rpx; */
}
.move-area{
  width:90%;
  position:absolute;
  top:0;
  left:50%;
  transform: translateX(-50%);
  z-index: 6;
}
.move-view{
  position:absolute;
  border:1px solid #fff;
  z-index:7;
}
.bottom-w{
  position:fixed;
  flex-direction: row;
  width:100%;
  bottom:0;
  height:120rpx;
  line-height: 120rpx;
  text-align: center;
  color:rgb(207, 207, 207);
  background-color:black;
  border-top:1px solid rgb(59, 59, 59);
  font-size:30rpx;
  z-index: 10;
}
.bottom-w view{
  float:left;
  
  text-align: center;
}

