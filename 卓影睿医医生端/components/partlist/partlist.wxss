.part {
  position:relative;
  z-index: 0;
  width: 200rpx;
  background-color: white;
  border-radius:6rpx;
  box-shadow:0px 0px 2px 2px  rgb(238,238,238);
}
.templatelabel{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.bottem{
  width: 100%;
  box-shadow:0px 0px 1px 1px  rgba(124,157,243, 0.1);
}
.part-radio {
  width: 100%;
  border: none;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: rgb(96,98,102);
  font-size: 24rpx;
}

.part-radio.active {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: rgb(241,242,243);
  font-size: 28rpx;
  color: rgb(34,34,34);
}