page {
  height: 100%;
  width: 100%;
  font-family: MicrosoftYahei;
}

.background {
  width: 100%;
  height: 100%;
  background-color: rgb(241, 242, 243);
}

.empty {
  position: absolute;
  top: 0;
  height: 100%;
  width: 100%;
  align-items: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.epimg {
  position: relative;
  left: 40rpx;
  width: 209rpx;
  height: 137rpx;
}

.emptytext {
  font-size: 24rpx;
  color: rgb(193, 193, 193);
}
.row image {
  width: 154rpx;
  height: 150rpx;
  margin-left: 30rpx;
}

.row {
  margin-top: 8rpx;
  width: 100%;
  height: 250rpx;
  background-image: url(https://wxprogram.zc5l.com/img/research__top_bg.png);
  background-size: 100% 100%;
  display: flex;
  align-items: center;
}

.icon {
  width: 154rpx;
  height: 150rpx;
  /* background-image: url(https://wxprogram.zc5l.com/img/doc_user_icon.png); */
  background-image: url(https://wxprogram.zc5l.com/img/Doc_male_icon.png);
  background-size: 100% 100%;
  margin-left: 30rpx;
}

.cbox {
  display: flex;
  flex-direction: column;
  margin-left: 36rpx;
}

.namebox {
  display: flex;
  justify-content: end;
}

.name {
  font-size: 36rpx;
  font-weight: bold;
  color: white;
  margin-right: 20rpx;
}

.profession {
  font-size: 24rpx;
  color: white;
}

.planrbox {
  display: flex;
}

.planvideobox {
  display: flex;
  margin-top: 16rpx;
}

.planbox {
  display: flex;
  width: 200rpx;
  align-items: center;
}

.property {
  font-size: 24rpx;
  color: white;
}

.value {
  margin-left: 20rpx;
  font-size: 24rpx;
  color: rgb(46, 218, 208);
  font-weight: bold;
}

.cvalue {
  font-size: 24rpx;
  color: rgb(46, 218, 208);
  font-weight: bold;
}

.line {
  height: 30rox;
  border-left: 1px solid rgb(124, 181, 251);
  margin-left: 32rpx;
  margin-right: 32rpx;
}

.hline {
  width: 30rpx;
  border-bottom: 2px solid rgb(46, 218, 208);
}

.cyclebox {
  margin-top: 18rpx;
  display: flex;
  align-items: center;
}

.statebox {
  position: fixed;
  top: 32rpx;
  right: 36rpx;
  display: flex;
  align-items: center;
}

.footmark {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 104rpx;
  background-image: url(https://wxprogram.zc5l.com/img/research_text_bg.png);
  background-size: 100% 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.questionnairebtn {
  color: rgb(46, 219, 208);
  font-size: 24rpx;
}

.questionnairebtnh {
  color: rgb(124, 181, 251);
}

.tx {
  margin-left: 17rpx;
  color: rgb(153, 153, 153);
  font-size: 24rpx;
}

.closebtn {
  position: fixed;
  right: 40rpx;
  bottom: 40rpx;
  width: 28rpx;
  height: 28rpx;
  background-image: url(https://wxprogram.zc5l.com/img/close_btn_n.png);
  background-size: 100% 100%;
}

.closebtnh {
  width: 28rpx;
  height: 28rpx;
  background-image: url(https://wxprogram.zc5l.com/img/close_btn_p.png);
  background-size: 100% 100%;
}

.bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 6px;
  background: linear-gradient(rgb(255, 255, 255), rgb(233, 233, 233));
}