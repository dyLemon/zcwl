page {
  height: 100%;
  width: 100%;
  font-family: MicrosoftYahei;
}

.titlebox {
  background-image: url(https://wxprogram.zc5l.com/img/Doclist_bg.png);
  background-size: 100% 100%;
  height: 160px;
  display: flex;
  justify-content: center;
}

.scanqr {
  margin-left: 20rpx;
  margin-top: 200rpx;
  width: 38rpx;
  height: 38rpx;
  background-image: url(https://wxprogram.zc5l.com/img/Scan_code_icon_n.png);
  background-size: 100% 100%;
}

.scanqrh {
  width: 38rpx;
  height: 38rpx;
  background-image: url(https://wxprogram.zc5l.com/img/Scan_code_icon_p.png);
  background-size: 100% 100%;
}

.searchbox {
  margin-top: 183rpx;
  width: 630rpx;
  height: 80rpx;
  background-color: white;
  border-radius: 80rpx;
  display: flex;
  align-items: center;
}

.searchicon {
  margin-left: 25rpx;
  width: 36rpx;
  height: 36rpx;
  background-image: url(https://wxprogram.zc5l.com/img/logon_search_icon.png);
  background-size: 100% 100%;
}

.searchinput {
  width: 450rpx;
  margin-left: 25rpx;
  font-size: 24rpx;
  color: rgb(102, 102, 102);
}

.clear {
  margin-left: 5rpx;
  width: 36rpx;
  height: 36rpx;
  background-image: url(https://wxprogram.zc5l.com/img/deleat_icon.png);
  background-size: 100% 100%;
}

.nopublicbox {
  position: absolute;
  top: 150px;
  width: 100%;
  height: 40px;
  background-color: rgb(248, 248, 248);
  display: flex;
  align-items: center;
}

.nopicon {
  margin-left: 40rpx;
  width: 32rpx;
  height: 32rpx;
  background-image: url(https://wxprogram.zc5l.com/img/deleat_icon.png);
  background-size: 100% 100%;
}

.infobox {
  margin-left: 11rpx;
  width: 384rpx;
  display: flex;
  flex-direction: column;
}

.pname {
  font-size: 24rpx;
  color: rgb(34, 34, 34);
}

.pdes {
  margin-top: 8rpx;
  font-size: 18rpx;
  color: rgb(119, 119, 119);
}

.attention {
  font-size: 24rpx;
  color: rgb(34, 34, 34);
  width: 120rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 50rpx;
  border: 1px solid rgb(119, 119, 119);
}

.closep {
  margin-left: 20rpx;
  width: 36rpx;
  height: 36rpx;
  background-image: url(https://wxprogram.zc5l.com/img/deleat_icon.png);
  background-size: 100% 100%;
}

.spcontentbg {
  width: 100%;
  background-color: white;
  position: absolute;
  top: 190px;
}

.contentbg {
  width: 100%;
  background-color: white;
  position: absolute;
  top: 150px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
}

.tablebox {
  width: 100%;
  height: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-bottom: 3px solid rgb(248, 248, 248);
}

.perinfobox {
  width: 690rpx;
  display: flex;
  height: 120rpx;
  border-bottom: 1px solid rgb(248, 248, 248);
}

.avatar {
  margin-top: 30rpx;
  width: 70rpx;
  height: 70rpx;
  background-size: 100% 100%;
}

.baseinfobox {
  margin-top: 32rpx;
  margin-left: 16rpx;
  display: flex;
  flex-direction: column;
}

.namebox {
  height: 30rpx;
  display: flex;
  align-items: flex-end;
}

.name {
  line-height: 30rpx;
  font-size: 30rpx;
  color: rgb(34, 34, 34);
}

.jobtitle {
  line-height: 24rpx;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: rgb(68, 68, 68);
}

.hospital {
  margin-top: 18rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: rgb(68, 68, 68);
}

.goodatbox {
  /* margin-top: 25rpx; */
  width: 100%;
  height: 60rpx;
  display: flex;
}

.goodaticon {
  margin-left: 60rpx;
  width: 30rpx;
  height: 30rpx;
  /* background-image: url(https://wxprogram.zc5l.com/img/GoodAt_icon.png); */
  background-size: 100% 100%;
}

.propertybox {
  margin-left: 26rpx;
  width: 560rpx;
  display: flex;
  align-items: center;
}

.property {
  position: absolute;
  font-size: 24rpx;
  color: rgb(34, 34, 34);
  line-height: 24rpx;
}

.goodat {
  position: relative;
  top: 0rpx;
  text-indent: 120rpx;
  text-align: left;
  font-size: 22rpx;
  color: rgb(119, 119, 119);
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.tagbox {
  display: flex;
  width: 100%;
}

.tagicon {
  margin-left: 60rpx;
  width: 30rpx;
  height: 30rpx;
  /* background-image: url(https://wxprogram.zc5l.com/img/Label_icon.png); */
  background-size: 100% 100%;
}

.tbox {
  margin-left: 26rpx;
  display: flex;

}

.tag {
  padding-left: 10rpx;
  padding-right: 10rpx;
  text-align: center;
  height: 40rpx;
  font-size: 24rpx;
  line-height: 40rpx;
  color: rgb(49, 220, 209);
  border-radius: 2px;
  background-color: rgb(234, 251, 250);
}

.empty {
  margin-top: 287rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.epimg {
  position: relative;
  left: 35rpx;
  width: 209rpx;
  height: 137rpx;
}